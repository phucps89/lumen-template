{
    "swagger": "2.0",
    "info": {
        "title": "Backend documented API",
        "version": "1.0.0"
    },
    "host": "",
    "basePath": "/",
    "schemes": [
        "http",
        "https"
    ],
    "paths": {
        "/account/user/profile": {
            "get": {
                "tags": [
                    "Account"
                ],
                "summary": "Get profile user logged in",
                "parameters": [
                    {
                        "$ref": "#/parameters/auth_header"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Get profile successfully"
                    }
                }
            }
        },
        "/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Login function",
                "parameters": [
                    {
                        "name": "credential",
                        "in": "body",
                        "description": "Password",
                        "required": true,
                        "schema": {
                            "properties": {
                                "username": {
                                    "type": "string"
                                },
                                "password": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Login successfully"
                    }
                }
            },
            "delete": {
                "tags": [
                    "Auth"
                ],
                "summary": "Logout function",
                "parameters": [
                    {
                        "$ref": "#/parameters/auth_header"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Logout successfully"
                    }
                }
            }
        }
    },
    "definitions": {
        "error_default_1": {
            "description": "Error of business (catch by developer)",
            "properties": {
                "_type": {
                    "description": "Error type",
                    "type": "string",
                    "default": "client_error"
                },
                "_time": {
                    "description": "Time response",
                    "type": "string",
                    "default": "2016-05-19 02:43:52"
                },
                "message": {
                    "description": "Text message",
                    "type": "string",
                    "default": ""
                }
            }
        },
        "error_default_2": {
            "description": "Error of logic (occurs on runtime)",
            "properties": {
                "_type": {
                    "description": "Error type",
                    "type": "string",
                    "default": "client_error"
                },
                "_time": {
                    "description": "Time response",
                    "type": "string",
                    "default": "2016-05-19 02:43:52"
                },
                "errors": {
                    "properties": {
                        "message": {
                            "description": "Message error",
                            "type": "string",
                            "default": "Error at ..."
                        },
                        "traces": {
                            "description": "Tracking error",
                            "type": "array",
                            "items": {
                                "type": "object"
                            }
                        }
                    },
                    "type": "object"
                }
            }
        }
    },
    "parameters": {
        "auth_header": {
            "name": "Authorization",
            "in": "header",
            "description": "Token",
            "required": true,
            "type": "string",
            "default": "Bearer "
        }
    }
}